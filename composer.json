{
    "name": "ksart/terma",
    "description": "Terma",
    "authors": [
        {
            "name": "Jeroen van der Steen",
            "email": "info@jeroensteen.nl"
        }
    ],
    "require": {
        "illuminate/database": "^5.4",
        "zoonman/pixabay-php-api": "^1.0",
        "emanueleminotto/simple-html-dom": "^1.5",
        "facebook/webdriver": "^1.4",
        "jonnyw/php-phantomjs": "4.*",
        "graphaware/neo4j-php-client": "^4.0"
    },
    "autoload": {
    "classmap": [
      "models",
      "jobs"
    ]
  },
  "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "config": {
        "bin-dir": "bin"
    }
}
